.lds-ring div { box-sizing: border-box !important; display: block !important; position: absolute !important; width: 64px !important; height: 64px !important; margin: 8px !important; border-width: 8px !important; border-style: solid !important; border-image: initial !important; border-radius: 50% !important; animation: 1.2s cubic-bezier(0.5, 0, 0.5, 1) 0s infinite normal none running lds-ring !important; border-color: rgb(95, 166, 36) rgba(95, 166, 36, 0.25) rgba(0, 0, 0, 0) transparent !important; }

.lds-ring div:nth-child(1) { animation-delay: -0.45s !important; }

.lds-ring div:nth-child(2) { animation-delay: -0.3s !important; }

.lds-ring div:nth-child(3) { animation-delay: -0.15s !important; }

@keyframes lds-ring { 
  0% { transform: rotate(0deg); }
  100% { transform: rotate(360deg); }
}

.ez-video-link { color: rgb(95, 166, 36); text-decoration: none; }

.ez-vid-placeholder { background-color: rgb(64, 61, 57) !important; }

.vjs-vtt-links-link a { display: flex !important; position: relative !important; justify-content: center !important; align-items: center !important; min-height: 40% !important; font-size: 15px !important; background-color: rgb(64, 61, 57) !important; opacity: 0.7 !important; transition: all 0.1s ease 0s !important; border-radius: 4px !important; }

[id^="ez-video-links-"] { min-height: 10vh !important; margin: 4px !important; }

.vjs-vtt-links-link svg { margin-left: 6px !important; margin-top: 6px !important; z-index: 3 !important; display: inline-block !important; stroke: rgba(0, 0, 0, 0) !important; fill: rgb(245, 245, 245) !important; color: rgb(245, 245, 245) !important; }

.vjs-vtt-links-link a:hover, .vjs-vtt-links-link a:hover svg { opacity: 1 !important; }

.vjs-has-started.vjs-user-inactive.vjs-playing + [id^="ez-video-links-"] .vjs-vtt-links-link svg, .vjs-has-started.vjs-user-inactive.vjs-playing + [id^="ez-video-links-"] .vjs-vtt-links-link a { opacity: 0 !important; transition: opacity 1s ease 0s !important; }

.vjs-vtt-links-link a, .vjs-vtt-links-link a:active, .vjs-vtt-links-link a:visited, .vjs-vtt-links-link a:hover, .vjs-vtt-links-link a:focus { color: rgb(95, 166, 36); }

