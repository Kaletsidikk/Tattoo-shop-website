  0%, 100% { transform: translate3d(0px, 0px, 0px); }
  10%, 30%, 50%, 70%, 90% { transform: translate3d(-10px, 0px, 0px); }
  20%, 40%, 60%, 80% { transform: translate3d(10px, 0px, 0px); }
}

.shake { animation-name: shake; }

